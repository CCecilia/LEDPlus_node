doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0', name='viewport')
		link(rel='apple-touch-icon', sizes='76x76', href='/images/ledPlusLogo.png')
		title= title
		//- Bootstrap CSS
		link(rel='stylesheet', type='text/css', href='/stylesheets/bootstrap.min.css')
		//-  Bootstrap Select CSS
		link(rel='stylesheet', type='text/css', href='/stylesheets/bootstrap-select.min.css')
		//- Datatables CSS
		link(rel='stylesheet', type='text/css', href='https://cdn.datatables.net/v/bs/jszip-2.5.0/dt-1.10.16/b-1.5.1/b-html5-1.5.1/fh-3.1.3/sl-1.2.5/datatables.min.css')
		//- jQuery UI CSS
		link(rel='stylesheet', type='text/css', href='/stylesheets/jquery-ui.min.css')
		//- Material Dashboard CSS
		link(rel='stylesheet', type='text/css', href='/stylesheets/material-dashboard.css')
		//- LED Plus CSS
		link(rel='stylesheet', type='text/css', href='/stylesheets/ledplus.css')
		//- Fonts and Icons
		link(rel='stylesheet', type='text/css', href='http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css')
		link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons')  
	body
		//- Side Navbar
		div.wrapper
			div.sidebar(data-color='green', data-image='/images/ledPlusLogo.png')
				div.logo
					a.simple-text(href='/') LED Plus
				div.sidebar-wrapper
					ul.nav
						if title == 'Login' || title == 'LED Plus'
							li.active
									a(href='/users/login') 
										i.material-icons lock_open
										p Log In
						else
							li(class=('Dashboard'===title ? 'active' : ''))
									a(href='/users/dashboard/5a7e269087cf600907bb3ae8/') 
										i.material-icons dashboard
										p Dashboard	
							li(class=('New Sale'===title ? 'active' : ''))
									a(href='/sales/new/') 
										i.material-icons assignment_turned_in
										p New Sale	
							li(class=('Sales'===title ? 'active' : ''))
									a(href='/sales/') 
										i.material-icons assessment
										p Sales		
							li(class=('Retail Energy Provider'===title ? 'active' : ''))
									a(href='/retailEnergyProviders/') 
										i.material-icons assessment
										p Retail Energy Provider	
							li(class=('LEDs'===title ? 'active' : ''))
									a(href='/LEDs') 
										i.material-icons assessment
										p LEDs		
							li(class=('Service Classes'===title ? 'active' : ''))
									a(href='/serviceClasses') 
										i.material-icons assessment
										p Service Classes	
			div.main-panel
				nav.navbar.navbar-transparent.navbar-absolute
					if title == 'Login' || title == 'LED Plus'
						div.container-fluid
							div.navbar-header
									button.navbar-toggle(type='button', data-toggle='collapse')
										span.sr-only Toggle navigation
										span.icon-bar
										span.icon-bar
										span.icon-bar
									a.navbar-brand(href='/') LED Plus
					else
						div.container-fluid
							div.navbar-header
									button.navbar-toggle(type='button', data-toggle='collapse')
										span.sr-only Toggle navigation
										span.icon-bar
										span.icon-bar
										span.icon-bar
									if user
										a.navbar-brand(href=user.dashboard) LED Plus
							div.collapse.navbar-collapse
									ul.nav.navbar-nav.navbar-right
										li
											if user
												a.dropdown-toggle(href='#', data-toggle="dropdown")
													i.material-icons dashboard
													p.hidden-lg.hidden-md Dashboard
												ul.dropdown-menu
													li.text-primary.text-center 
														p #{user.email}
													li
														a.dropdown-toggle(href='/users/logout')
															.row
																.col-md-6.text-left Log out
																.col-md-6.text-right
																	i.material-icons exit_to_app
				block content
				footer.footer
					div.container-fluid
						nav.pull-left
							ul
								li
									if user
										a(href=user.dashboard) LED Plus
									else
										a(href='/') LED Plus
						p.copyright.pull-right
							| Â©
							script.
								document.write(new Date().getFullYear())
							a(href='http://christiancecilia.us-east-1.elasticbeanstalk.com/') Christian Cecilia
	//- Core JS
	script(type='text/javascript', src='/javascripts/jquery-3.2.1.min.js') 
	script(type='text/javascript', src='/javascripts/bootstrap.min.js') 
	script(type='text/javascript', src='/javascripts/material.min.js') 
	//- Charts Plugin
	script(type='text/javascript', src='/javascripts/chartist.min.js') 
	//- Dynamic Elements plugin
	script(type='text/javascript', src='/javascripts/arrive.min.js')
	//- PerfectScrollbar Library
	script(type='text/javascript', src='/javascripts/perfect-scrollbar.jquery.min.js')
	//- Notifications Plugin 
	script(type='text/javascript', src='/javascripts/bootstrap-notify.js')
	//- Bootstrap Select Plugin
	script(type='text/javascript', src='/javascripts/bootstrap-select.js')
	//- Datatables Library
	script(type='text/javascript', src='https://cdn.datatables.net/v/bs/jszip-2.5.0/dt-1.10.16/b-1.5.1/b-html5-1.5.1/fh-3.1.3/sl-1.2.5/datatables.min.js')
	//- jQuery UI Plugin
	script(type='text/javascript', src='/javascripts/jquery-ui.min.js')
	//- Material Dashboard javascript methods
	script(type='text/javascript', src='/javascripts/material-dashboard.js')
	//- Underscore Library
	script(type='text/javascript', src='/javascripts/underscore.js')
	//- DropZoneJS Plugin
	script(type='text/javascript', src='/javascripts/dropzone.js')
	//- PapaParse Plugin
	script(type='text/javascript', src='/javascripts/papaparse.min.js')
	//- Google Maps
	script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDJFB9TXQP3JKkeq6NnrMeby-8HNB2GGh4&amp;libraries=places&amp;callback=initAutocomplete", async="", defer="")
	//- Global 
	script(type='text/javascript', src='/javascripts/global.js')
